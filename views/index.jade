doctype html
html
  head
    title Exchange
    link(href='/bin/css/bootstrap.min.css', rel='stylesheet')
    script(src='/bin/js/jquery-2.1.4.min.js')
    script(src='/bin/js/bootstrap.min.js')

    link(href='/bin/css/index.css' rel='stylesheet')
